<%= form_with model: @post do |m| %>
    <p>タイトル　<%= m.text_field :title %></p>

    <p>　　内容　<%= m.text_area :body %></p>

    <p>　言語名　<%= m.collection_select :language_id, Language.all, :id, :lang_name %></p>

    <p>　　画像　<%= m.file_field :images, multiple: true %></p>

    <p>　　　　　<%= m.submit %></p>
<% end %>
